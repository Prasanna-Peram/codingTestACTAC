v>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      books: [],\r\n      newBook: { title: '', author: '', published_year: '' }\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchBooks();\r\n  },\r\n  methods: {\r\n    fetchBooks() {\r\n      axios.get('http://localhost/fullstack/backend/index.php/books')\r\n            .then(response => {\r\n            this.books = response.data;\r\n        })\r\n        .catch(error => {\r\n            console.error('There was an error!', error);\r\n        });\r\n    },\r\n    addBook() {\r\n      axios.post('http://localhost/fullstack/backend/index.php/books/create', this.newBook)\r\n        .then(() => {\r\n          this.fetchBooks();\r\n          this.newBook = { title: '', author: '', published_year: '' };\r\n        })\r\n         .catch(error => {\r\n            console.error('There was an error!', error);\r\n        });\r\n        \r\n    },\r\n    deleteBook(id) {\r\n      axios.delete(`http://localhost/fullstack/backend/index.php/books/delete/${id}`)\r\n        .then(() => {\r\n          this.fetchBooks();\r\n        })\r\n         .catch(error => {\r\n            console.error('There was an error!', error);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":"AAiBA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,OAAA;QAAAC,KAAA;QAAAC,MAAA;QAAAC,cAAA;MAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,UAAA;EACA;EACAC,OAAA;IACAD,WAAA;MACAR,KAAA,CAAAU,GAAA,uDACAC,IAAA,CAAAC,QAAA;QACA,KAAAV,KAAA,GAAAU,QAAA,CAAAX,IAAA;MACA,GACAY,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,wBAAAA,KAAA;MACA;IACA;IACAE,QAAA;MACAhB,KAAA,CAAAiB,IAAA,mEAAAd,OAAA,EACAQ,IAAA;QACA,KAAAH,UAAA;QACA,KAAAL,OAAA;UAAAC,KAAA;UAAAC,MAAA;UAAAC,cAAA;QAAA;MACA,GACAO,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,wBAAAA,KAAA;MACA;IAEA;IACAI,WAAAC,EAAA;MACAnB,KAAA,CAAAoB,MAAA,8DAAAD,EAAA,IACAR,IAAA;QACA,KAAAH,UAAA;MACA,GACAK,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,wBAAAA,KAAA;MACA;IACA;EACA;AACA","ignor